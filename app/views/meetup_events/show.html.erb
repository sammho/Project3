<p id="notice"><%= notice %></p>

<table>
  <tr>
    <th></th>
    <th>Name</th>
    <th>Twitter</th>
    <th>LinkedIn</th>
    <th>Topics in Common</th>
    <th></th>
  </tr>

<% @rsvpd_members.each do |meetup_member| %>
  <tr>
    <td><%= image_tag(meetup_member.image_url, :border => 0, :width => "32") %></td>
    <td><%= link_to meetup_member.name, "http://www.meetup.com/members/#{meetup_member.meetup_id}" %></td>
    <% if meetup_member.twitter%>
      <td><%= link_to meetup_member.twitter, "http://twitter.com/#{meetup_member.twitter}" %> </td>
    <% else %>
      <td></td>
    <% end %>
    <% if meetup_member.linkedin_url %>
      <td><%= link_to "LinkedIn", meetup_member.linkedin_url %> </td>
    <% else %>
      <td></td>
    <% end %>
    <td><%= current_user.topics_in_common(meetup_member).count %></td>
    <td><%= meetup_member.unparsed_json %></td>
  </tr>
<% end %>
</table>

<%=@rsvpd_members %>
